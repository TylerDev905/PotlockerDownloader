{
	"manifest_version": 2,
	"name": "Potlocker Scraper",
	"description": "A program that will scrape links from potlocker.me",
	"version": "1.0",
	"permissions": [
		"storage",
        "http://potlocker.me//"
	],
	"icons": { 
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png" 
	},
	"browser_action": {
		"default_icon": {                    
			"19": "img/icon48.png",          
			"38": "img/icon48.png"          
		},
		"default_popup": "popup.html"
	},
	"web_accessible_resources": [
		"img/icon128.png"
	]
}